<UserControl x:Class="ElementsForInterface.CustomBtn"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ElementsForInterface"
             mc:Ignorable="d" 
             d:DesignHeight="38.55" d:DesignWidth="300">
    <UserControl.Resources>
        
        <FontFamily x:Key="JuraFontFamily">/Fonts/#Jura</FontFamily>
    </UserControl.Resources>
    <Grid>
        <Button Background="#3D95B9" x:Name="Btn" Width="300px" Height="38.55px">
        </Button>
    </Grid>
</UserControl>
